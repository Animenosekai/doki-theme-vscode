{
	"name": "doki-theme",
	"displayName": "doki-theme-vscode",
	"description": "A bunch of character-based themes.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.Satsuki",
		"onCommand:extension.Ryuko"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.Satsuki",
				"title": "Satsuki Kiruin"
			},
			{
				"command": "extension.Ryuko",
				"title": "Ryuko Matoi"
			}
		],
		"themes": [
			{
				"id": "3a78b13e-dbf2-410f-bb20-12b57bff7735",
				"label": "Satsuki Kiriun",
				"path": "./out/Satsuki.theme.json",
				"uiTheme": "vs"
			},
			{
				"id": "19b65ec8-133c-4655-a77b-13623d8e97d3",
				"label": "Ryuko Matoi",
				"path": "./out/Ryuko.theme.json",
				"uiTheme": "vs-dark"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^13.5.3",
		"@types/vscode": "^1.40.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	}
}
