{
    "name": "base",
    "tokenColors": [
        {
            "scope": [
                "support.type.property-name.json.comments",
                "meta.object-literal.key",
                "variable.object.property"
            ],
            "settings": {
                "foreground": "&foregroundColorEditor&",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "source"
            ],
            "settings": {
                "foreground": "&foregroundColorEditor&"
            }
        },
        {
            "scope": [
                "string.quoted",
                "variable.parameter"
            ],
            "settings": {
                "foreground": "&stringColor&"
            }
        },
        {
            "scope": [
                "constant.language.json",
                "keyword",
                "storage.type",
                "constant.language.boolean"
            ],
            "settings": {
                "foreground": "&keywordColor&"
            }
        },
        {
            "scope": [
                "variable.other.constant",
                "variable.other.readwrite"
            ],
            "settings": {
                "foreground": "&classNameColor&"
            }
        },
        {
            "scope": [
                "keyword.operator"
            ],
            "settings": {
                "foreground": "&htmlTagColor&"
            }
        },
        {
            "scope": [
                "entity.name.type.class",
                "meta.type",
                "variable.other.object"
            ],
            "settings": {
                "foreground": "&keyColor&"
            }
        },
        {
            "scope": [
                "meta.function-call"
            ],
            "settings": {
                "foreground": "&accentColor&"
            }
        }
    ]
}